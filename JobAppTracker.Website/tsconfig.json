{
	"compilerOptions": {
		"outDir": "./wwwroot/js",
		"sourceMap": true,
		"allowJs": true,
		"lib": [
			"dom",
			"es5",
			"scripthost",
			"es2015.iterable"
		],
		"target": "es5",
		"module": "commonjs",
					"moduleResolution": "node",
					"skipDefaultLibCheck": true,
					"types": ["es6-promise", "history", "requirejs"]
	},
	"include": [
		"./ts/**/*"
	]
}